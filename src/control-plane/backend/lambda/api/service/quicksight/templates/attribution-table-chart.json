{
  "TableVisual": {
    "VisualId":"{{visualId}}",
    "Title":{
        "FormatText": {
            "PlainText": "{{title}}"
        },
        "Visibility":"VISIBLE"
    },
    "Subtitle":{
        "FormatText": {
            "PlainText": "{{subTitle}}"
        },
        "Visibility":"VISIBLE"
    },
      "ChartConfiguration": {
          "FieldWells": {
              "TableAggregatedFieldWells": {
                  "GroupBy": [
                      {
                        "CategoricalDimensionField": {
                            "FieldId": "{{touchPointNameFieldId}}",
                            "Column": {
                                "DataSetIdentifier": "{{dataSetIdentifier}}",
                                "ColumnName": "Touch Point Name"
                            }
                        }
                      },
                      {
                          "NumericalDimensionField": {
                            "FieldId": "{{totalTriggerCountFieldId}}",
                            "Column": {
                                "DataSetIdentifier": "{{dataSetIdentifier}}",
                                "ColumnName": "Trigger Count"
                            }
                          }
                      },
                      {
                          "NumericalDimensionField": {
                              "FieldId": "{{triggerCountFieldId}}",
                              "Column": {
                                  "DataSetIdentifier": "{{dataSetIdentifier}}",
                                  "ColumnName": "Number of Attributed Triggers"
                              }
                          }
                      },
                      {
                        "NumericalDimensionField": {
                            "FieldId": "{{contributionFieldId}}",
                            "Column": {
                                "DataSetIdentifier": "{{dataSetIdentifier}}",
                                "ColumnName": "Contribution"
                            }
                        }
                    },
                      {
                          "NumericalDimensionField": {
                              "FieldId": "{{contributionRateFieldId}}",
                              "Column": {
                                  "DataSetIdentifier": "{{dataSetIdentifier}}",
                                  "ColumnName": "Contribution Rate"
                              },
                              "FormatConfiguration": {
                                "FormatConfiguration": {
                                    "PercentageDisplayFormatConfiguration": {
                                        "Suffix": "%",
                                        "SeparatorConfiguration": {
                                            "DecimalSeparator": "DOT",
                                            "ThousandsSeparator": {
                                                "Symbol": "COMMA",
                                                "Visibility": "VISIBLE"
                                            }
                                        },
                                        "DecimalPlacesConfiguration": {
                                            "DecimalPlaces": 2
                                        },
                                        "NegativeValueConfiguration": {
                                            "DisplayMode": "NEGATIVE"
                                        }
                                    }
                                }
                            }
                          }
                      } 
                  ],
                  "Values": []
              }
          },
          "SortConfiguration": {
            "RowSort": [
              {
                  "FieldSort": {
                      "FieldId": "{{touchPointNameFieldId}}",
                      "Direction": "ASC"
                  }
              }
          ]
          },
          "TableOptions": {
              "CellStyle": {
                  "Height": 25
              }
          }
      },
      "Actions": []
  }
}